<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_bezier"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.shize.listdemo.activity.BezierActivity">

    <com.example.shize.listdemo.view.BezierTestView
        android:id="@+id/id_bezier_test"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible" />

    <com.example.shize.listdemo.view.BezierDrawPadView
        android:id="@+id/id_bezier_pad"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <com.example.shize.listdemo.view.BezierAnimView
        android:id="@+id/id_bezier_anim"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.example.shize.listdemo.view.BezierWaveView
        android:id="@+id/id_bezier_wave"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.example.shize.listdemo.view.BezierPathAnimView
        android:id="@+id/id_bezier_path"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

</FrameLayout>
